{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json2",
    "type": "object",
    "title": "User schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "id": 12,
            "firstName": "Bob",
            "lastName": "Fellow",
            "email": "bob.fellow@gmail.com",
            "password": "123456",
            "username": "bob"
        }
    ],
    "required": [
        "id",
        "firstName",
        "lastName",
        "email",
        "password",
        "username"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                12
            ]
        },
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "The firstName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Bob"
            ]
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "The lastName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Fellow"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "bob.fellow@gmail.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "123456"
            ]
        },
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "bob"
            ]
        }
    },
    "additionalProperties": true
}